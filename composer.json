{
    "name": "kirtan/backup",
    "description": "This will create your database backup when you run the command",
    "keywords": ["framework", "laravel", "database", "database backup", "backup"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":"Kirtan",
            "email":"kirtan@gmail.com"
        }
    ],

    "minimum-stability": "dev",
    "prefer-stable": true,

    "require": {
        "php": "^8.0",        
        "laravel/framework": "^9.0"
    },

    "extra": {
        "laravel": {
            "providers": [
                "Kirtan\\Backup\\BackupServiceProvider"
            ]
        }
    },

    "autoload": {
        "psr-4": {
            "Kirtan\\Backup\\":"src/"
        }
    }
}
